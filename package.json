{
  "name": "impress.js",
  "version": "1.0.0",
  "description": "impress.js is a server side HTML based template markup that can be extended by reporters",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --recursive -G -u bdd -c -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "posttest": "./node_modules/.bin/istanbul check-coverage",
    "testwin": "eslint . && mocha --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khrome83/impressjs.git"
  },
  "keywords": [
    "template",
    "templates",
    "dom",
    "views",
    "html"
  ],
  "author": "Zane C. Milakovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khrome83/impressjs/issues"
  },
  "homepage": "https://github.com/khrome83/impressjs#readme",
  "dependencies": {
    "lodash": "^3.10.1",
    "parse5": "^2.0.2",
    "parse5-utils": "^1.4.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "mockery": "^1.4.0",
    "proxyquire": "^1.7.3",
    "rewire": "^2.5.1",
    "sinon": "^1.17.2"
  }
}
