{
  "name": "impress.js",
  "version": "1.0.0",
  "description": "impress.js is a server side HTML based template markup that can be extended by reporters",
  "main": "lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint ./lib/ ./test/ gulpfile.js --reporter=node_modules/jshint-stylish",
    "checkStyle": "./node_modules/.bin/jscs ./lib/ ./test/ gulpfile.js --reporter=node_modules/jscs-stylish",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/mocha --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khrome83/impressjs.git"
  },
  "keywords": [
    "template",
    "templates",
    "dom",
    "views",
    "html"
  ],
  "author": "Zane C. Milakovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khrome83/impressjs/issues"
  },
  "homepage": "https://github.com/khrome83/impressjs#readme",
  "dependencies": {
    "lodash": "^3.10.1",
    "parse5": "^1.5.0",
    "parse5-utils": "^1.4.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.1.1",
    "gulp-jscs": "^3.0.2",
    "gulp-jshint": "^2.0.0",
    "gulp-plumber": "^1.0.1",
    "istanbul": "^0.4.1",
    "jscs-stylish": "^0.3.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0",
    "mocha": "^2.3.4",
    "plumber": "^0.4.8"
  }
}
